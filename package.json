{
  "name": "@subroutinecom/workspace",
  "version": "0.1.13",
  "description": "Self-contained CLI for spinning up Docker-in-Docker workspaces with SSH and proxy helpers.",
  "bin": {
    "workspace": "./dist/index.js"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc && chmod +x dist/index.js && npm run build:web",
    "build:web": "cd web && npm run build && cp -r dist ../dist/agent/web",
    "test": "vitest run",
    "test:web": "playwright test",
    "test:watch": "vitest",
    "lint": "oxlint src/",
    "lint:fix": "oxlint --fix src/",
    "format": "oxfmt --write src/ test/",
    "format:check": "oxfmt --check src/ test/",
    "check": "npm run lint && npm run format:check && tsc --noEmit"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@opentui/core": "^0.1.67",
    "@orpc/server": "^1.13.2",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "ora": "^5.4.1",
    "proper-lockfile": "^4.1.2",
    "update-notifier": "^7.3.1",
    "ws": "^8.18.3",
    "yaml": "^2.3.4",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.8.1",
    "@types/proper-lockfile": "^4.1.4",
    "@types/update-notifier": "^6.0.8",
    "@types/ws": "^8.18.1",
    "oxfmt": "^0.21.0",
    "oxlint": "^1.36.0",
    "typescript": "^5.6.3",
    "vitest": "^4.0.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/subroutinecom/workspace.git"
  },
  "keywords": [
    "docker",
    "development",
    "workspace",
    "cli",
    "docker-in-docker",
    "ssh"
  ],
  "author": "Subroutine",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
